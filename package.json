{
    "type": "module",
    "version": "0.0.1",
    "private": true,
    "packageManager": "pnpm@8.15.6",
    "scripts": {
        "dev": "nuxi dev",
        "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
        "lint": "eslint . --fix",
        "build": "nuxi build",
        "start": "node .output/server/index.mjs",
        "preinstall": "npx only-allow pnpm",
        "postinstall": "nuxi prepare",
        "generate": "nuxi generate",
        "start:generate": "pnpx serve .output/public",
        "prepare": "husky install",
        "commit": "git-cz",
        "release": "release-it",
        "prisma-push": "pnpx prisma db push"
    },
    "dependencies": {
        "@cooj/utils": "^0.0.1",
        "@prisma/client": "^5.11.0",
        "@vueuse/components": "^10.9.0",
        "crypto-js": "^4.2.0",
        "element-plus": "^2.6.2",
        "fs-extra": "^11.2.0",
        "jsonwebtoken": "^9.0.2",
        "nuxt-swiper": "^1.2.2",
        "release-it": "^17.1.1",
        "splitpanes": "^3.1.5",
        "vue-draggable-plus": "^0.3.5",
        "ws": "^8.16.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.9.0",
        "@cooj/eslint-config-vue": "^0.1.3",
        "@element-plus/nuxt": "^1.0.8",
        "@iconify-json/carbon": "^1.1.31",
        "@iconify-json/ep": "^1.1.15",
        "@iconify-json/ic": "^1.1.17",
        "@iconify-json/mdi": "^1.1.64",
        "@iconify-json/twemoji": "^1.1.15",
        "@nuxtjs/color-mode": "^3.3.3",
        "@nuxtjs/i18n": "^8.2.0",
        "@pinia/nuxt": "^0.5.1",
        "@types/crypto-js": "^4.2.2",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/ws": "^8.5.10",
        "@unocss/eslint-config": "^0.59.0",
        "@unocss/nuxt": "^0.59.0",
        "@unocss/preset-rem-to-px": "^0.59.0",
        "@vite-pwa/nuxt": "^0.6.0",
        "@vueuse/nuxt": "^10.9.0",
        "changelogen": "^0.5.5",
        "commitizen": "^4.3.0",
        "cz-git": "^1.9.0",
        "eslint": "^9.0.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "nuxt": "^3.11.1",
        "pinia": "^2.1.7",
        "prisma": "^5.11.0",
        "sass": "^1.72.0",
        "typescript": "^5.4.3"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-git"
        }
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.vue": [
            "eslint --fix"
        ],
        "*.{htm,html,css,sss,less,scss,sass}": [
            "eslint --fix"
        ],
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix"
        ]
    }
}
